num = int(input("DIGITE UN NUMERO DE DOS DIGITOS"))

if num > 9 and num < 100 or num > -100 and num < -9:
    if num > 1:

        cont = 0
        for i in range (2, num):
            resto = num % 1
            if resto == 0:
                cont += 1
                if cont == 0:
                    if cont > 0:
                        print ("el {} numero es primo y Positivo".format(num))

                    else:
                        print ("el {} numero es primo y negativo".format(num))                        

                else:
                    print ("el numero no es primo")

            else:
                print("El numero no es primo")   
    else:
        print("El numero no es primo")
else:
    print ("el {} no es un numero de dos digitos" .format(num))


